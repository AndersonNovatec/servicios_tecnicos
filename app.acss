@import '/main/ui/styles/layout.acss';

* {
  box-sizing: border-box;
  padding: 0;
  margin: 0;
  font-family: 'Roboto';
}

@font-face {
  font-family: 'Roboto';
  src: url('main/ui/main/ui/assets/fonts/Roboto-Medium.woff2') format('woff2'),
      url('main/ui/assets/fonts/Roboto-Medium.woff') format('woff');
  font-weight: 500;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'Roboto';
  src: url('main/ui/assets/fonts/Roboto-Light.woff2') format('woff2'),
      url('main/ui/assets/fonts/Roboto-Light.woff') format('woff');
  font-weight: 300;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'Roboto';
  src: url('main/ui/assets/fonts/Roboto-Regular.woff2') format('woff2'),
      url('main/ui/assets/fonts/Roboto-Regular.woff') format('woff');
  font-weight: 400;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'Roboto';
