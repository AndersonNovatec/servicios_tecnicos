  --c60-808080: #808080;
  --c70-5e5e5e: #5E5E5E;
  --c20-cccccc: #CCCCCC;
  --rojo-base-boton-da291c: #DA291C;
  --azul-120-10505c: #10505C;
  --c80-404040: #404040;
