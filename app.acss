  font-display: swap;
}

@font-face {
  font-family: 'AMX';
  src: url('main/ui/assets/fonts/AMX-Regular.woff2') format('woff2'),
      url('main/ui/assets/fonts/AMX-Regular.woff') format('woff');
  font-weight: 400;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'AMX';
  src: url('main/ui/assets/fonts/AMX-Medium.woff2') format('woff2'),
      url('main/ui/assets/fonts/AMX-Medium.woff') format('woff');
